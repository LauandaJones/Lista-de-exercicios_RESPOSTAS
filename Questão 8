import java.util.Scanner;
public class Main
{
          public static void main( String Args[ ] )
          {
              Scanner leitor = new Scanner(System.in);
              
                 int fibonacci = 0;
                 int numero = leitor.nextInt();
                 for ( int i = 1; i <=numero; i ++ )
                {
                    fibonacci = ( i - 1 ) + ( i - 2 );
                    System.out.println( fibonacci );

                }
         }
}
