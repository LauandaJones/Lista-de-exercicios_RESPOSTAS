import java.util.Scanner;

public class Main
{
	public static void main(String[] args) {
	    Scanner leitor = new  Scanner(System.in);
	    
	    System.out.println("Valores do primeiro ponto: ");
	    double XA = leitor.nextDouble();
	    double YA = leitor.nextDouble();
	    
	    System.out.println("valores do segundo ponto: ");
	    double XB = leitor.nextDouble();
	    double YB = leitor.nextDouble();
	    
	    double VA = Math.pow((XB - XA), 2);
	    double VB = Math.pow((YB - YA), 2);
	    double VT = VA + VB;
	    
	    double RESULTADO = Math.sqrt(VT);
	    
		System.out.println("A distância é: " + RESULTADO);
	}
}
